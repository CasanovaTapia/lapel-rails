<%= render "client/navbar" %>
<div class="client-content text-center">
	<div class="request row">
			<div class="col-sm-6">
				<%= link_to(new_user_appointment_path(current_user)) do %>
					<div class="req-box">
						<%= image_tag('fixed-wardrobe.svg', height: "190") %><br/>
						<h6>Schedule a styling</h6>
					</div>
				<% end %>
			</div>
			<div class="col-sm-6">
				<%= link_to(new_user_order_path(current_user)) do %>
					<div class="req-box">
						<%= image_tag('Outfits.png') %><br/>
						<h6>Submit an order</h6>
					</div>
				<% end %>
			</div>
	</div>
</div>
